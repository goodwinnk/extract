---
extracts:
  - name: cli
    files: [
      "cli/**",
    ]
    style: e3

  - name: core
    files: [
      "core/**",
    ]
    style: e3

  - name: core-common
    files: [
      "core-common/**",
    ]
    style: e3

  - name: core-js
    files: [
      "core-common/core-js/**",
    ]
    style: e3

  - name: core-jvm
    files: [
      "core-common/core-jvm/**",
    ]
    style: e3

  - name: chrome
    files: [
      "extensions/chrome/**",
    ]
    style: e3

  - name: manifest.json
    files: [
      "**/manifest.json",
    ]
    style: e3

  - name: demo
    files: [
      "demo/**",
    ]
    style: e3

  - name: docs
    files: [
      "docs/**",
    ]
    style: e3

  - name: build
    files: [
      "**.gradle",
    ]
    style: e4

  - name: .idea
    files: [
      ".idea/**",
    ]
    style: e4

  - name: npm
    files: [
      "**/package.json",
      "**/package-lock.json",
    ]
    style: e4

  - name: webpack
    files: [
      "**/webpack.config.js",
    ]
    style: e4

  - name: tsconfig
    files: [
      "**/tsconfig.json"
    ]
    style: e4

  - name: .extracts
    files: [
      ".extracts",
    ]
    style: e2

  - name: tests
    files: [
      "**/src/test/**",
      "**.spec.ts"
    ]
    style: e6

  - name: README
    files: [
      "README.md"
    ]
    style: e2

  - name: .travis
    files: [
      ".travis.yml"
    ]
    style: e2
---